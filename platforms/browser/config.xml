<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pinkbot.remote" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PinkRemote</name>
    <description>
        This is the project created from the pinkBot control block
    </description>
    <author email="dev@pinkbot.in" href="http://pinkbot.in">
        PinkBot Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
